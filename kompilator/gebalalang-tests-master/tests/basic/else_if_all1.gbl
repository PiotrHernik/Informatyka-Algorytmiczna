#pragma test in(12, -3) out(1,1,2,1,2,2,1,1,-2,-1,-1,-1,-2,-2)

PROCEDURE test(a,b,c) IS
BEGIN
    IF a >= 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF a > 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF a = 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF a != 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF a < 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF a <= 0 THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF


    IF 0 >= b THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF 0 > b THEN
        WRITE 1;
    ELSE
        WRITE 2;
    ENDIF

    IF 0 = c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF

    IF 0 != c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF

    IF 0 < c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF

    IF 0 <= c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF

    IF 0 > c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF

    IF 0 >= c THEN
        WRITE -1;
    ELSE
        WRITE -2;
    ENDIF
END

PROGRAM IS
    a, b, c
BEGIN
    a := 12;
    b := -3;
    c := a + b;
    test(a,b,c);
    
END